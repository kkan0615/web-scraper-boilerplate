{
  "name": "vue3-test",
  "author": "Youngin Kwak",
  "version": "0.0.5",
  "private": true,
  "main": "./dist/main/main.cjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node scripts/devServer.ts",
    "dev:renderer": "vite packages/renderer",
    "build:test": "vite packages/renderer",
    "build:window": "ts-node scripts/build.ts && electron-builder",
    "release": "ts-node scripts/build.ts && electron-builder --publish=always",
    "test": "vitest",
    "test:once": "vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "eslint:fix": "eslint --fix ./**/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kkan0615/web-scraper-boilerplate"
  },
  "build": {
    "appId": "com.app.web.scrapper",
    "productName": "Web-scrapper-boilerplate",
    "copyright": "Copyright Â© 2022 ${author}",
    "publish": [
      {
        "provider": "github",
        "private": true,
        "owner": "kkan0615",
        "repo": "\nweb-scraper-boilerplate"
      }
    ],
    "mac": {
      "icon": "./public/logo/logo_color-128.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./public/logo/logo_color-256.png",
      "publish": [
        "github"
      ],
      "target": [
        "zip",
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*"
    ],
    "extraResources": [
      "public",
      "teamlates"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "@vueuse/core": "^9.0.2",
    "@vueuse/electron": "^9.0.2",
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.0.5",
    "exceljs": "^4.3.0",
    "node-schedule": "^2.1.0",
    "pinia": "^2.0.24",
    "puppeteer": "^19.2.2",
    "quasar": "^2.10.2",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.2",
    "vue-router": "4",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@iconify/vue": "^4.0.0",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/electron": "^1.6.10",
    "@types/node": "^17.0.41",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "eslint": "8.22.0",
    "eslint-plugin-vue": "^9.1.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "sass": "1.32.12",
    "ts-node": "^10.8.2",
    "typescript": "^4.5.4",
    "vite": "^3.0.0",
    "vitest": "^0.20.2",
    "vue-tsc": "^0.34.7"
  }
}
